{
  "version": 2,
  "buildCommand": "npm run build",
  "functions": {
    "api/search.js": {
      "runtime": "nodejs18.x"
    },
    "api/convert.js": {
      "runtime": "nodejs18.x"
    },
    "api/health.js": {
      "runtime": "nodejs18.x"
    },
    "api/units/[id].js": {
      "runtime": "nodejs18.x"
    },
    "api/units/category/[category].js": {
      "runtime": "nodejs18.x"
    },
    "api/submit.js": {
      "runtime": "nodejs18.x"
    },
    "api/vote.js": {
      "runtime": "nodejs18.x"
    },
    "api/submissions/pending.js": {
      "runtime": "nodejs18.x"
    }
  },
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search.js"
    },
    {
      "src": "/api/convert",
      "dest": "/api/convert.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/units/([^/]+)",
      "dest": "/api/units/[id].js"
    },
    {
      "src": "/api/units/category/([^/]+)",
      "dest": "/api/units/category/[category].js"
    },
    {
      "src": "/api/submit",
      "dest": "/api/submit.js"
    },
    {
      "src": "/api/vote",
      "dest": "/api/vote.js"
    },
    {
      "src": "/api/submissions/pending",
      "dest": "/api/submissions/pending.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DB_PATH": "/tmp/unit_decoder.db"
  }
}
