{
  "version": 2,
  "buildCommand": "npm run build",
  "routes": [
    {
      "src": "/api/search",
      "dest": "/api/search-simple.js"
    },
    {
      "src": "/api/convert",
      "dest": "/api/convert.js"
    },
    {
      "src": "/api/health",
      "dest": "/api/health.js"
    },
    {
      "src": "/api/test",
      "dest": "/api/test.js"
    },
    {
      "src": "/api/units/([^/]+)",
      "dest": "/api/units/[id].js"
    },
    {
      "src": "/api/units/category/([^/]+)",
      "dest": "/api/units/category/[category].js"
    },
    {
      "src": "/api/submit",
      "dest": "/api/submit.js"
    },
    {
      "src": "/api/vote",
      "dest": "/api/vote.js"
    },
    {
      "src": "/api/submissions/pending",
      "dest": "/api/submissions/pending.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "DB_PATH": "/tmp/unit_decoder.db"
  }
}
